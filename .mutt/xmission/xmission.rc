bind editor <Tab> complete
bind editor ^T complete-query
unset query_command imap_user imap_pass
set alias_file="~/.mutt/xmission/xmission_aliases"
set imap_check_subscribed
set postponed="+Drafts"
set realname="Aaron Toponce"
set record="+Sent"
set reverse_alias=yes
set signature="~/.mutt/xmission/signature.txt"
set sort_alias=alias
set ssl_starttls="yes"
set ssl_force_tls="yes"
source $alias_file
source ~/.mutt/xmission/xmission_lists
source "gpg2 --no-tty -qd ~/.mutt/xmission/xmission.pass.gpg |"
# vim: set ft=muttrc:
